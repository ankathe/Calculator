        "# error_temp['caliente'] = fuzz.trimf(error_temp.universe, [0, 10, 10])\n",
        "\n",
        "error_temp.automf(names=['frio', 'ok', 'caliente']) # Crea 3 MFs triangulares\n",
        "\n",
        "# Para la potencia, vamos a definirlas manualmente para más control\n",
        "potencia_calef['baja'] = fuzz.trimf(potencia_calef.universe, [0, 0, 50])\n",
        "potencia_calef['media'] = fuzz.trimf(potencia_calef.universe, [25, 50, 75])\n",
        "potencia_calef['alta'] = fuzz.trimf(potencia_calef.universe, [50, 100, 100])\n",
        "\n",
