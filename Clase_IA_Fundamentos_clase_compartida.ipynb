        "# Celda de Código: Proyecto Unificador - Control de Nivel de Tanque\n",
        "\n",
        "# 1. Definir Antecedentes y Consecuentes\n",
        "universo_err_nivel = np.arange(-1, 1.1, 0.1) # Error de -1m a +1m\n",
        "universo_tasa_err = np.arange(-0.2, 0.21, 0.01) # Tasa de cambio de -0.2m/s a +0.2m/s\n",
        "universo_flujo = np.arange(0, 101, 1) # Flujo de bomba de 0% a 100%\n",
        "\n",
        "err_nivel = ctrl.Antecedent(universo_err_nivel, 'error_nivel')\n",
        "tasa_err = ctrl.Antecedent(universo_tasa_err, 'tasa_cambio_error')\n",
        "flujo_bomba = ctrl.Consequent(universo_flujo, 'flujo_bomba', defuzzify_method='centroid')\n",
        "\n",
        "# 2. Definir Funciones de Pertenencia\n",
        "# Para error_nivel (Negativo significa nivel por ENCIMA del setpoint, Positivo por DEBAJO)\n",
        "# NB: Muy_bajo significa que el nivel del tanque está muy por debajo del setpoint (error positivo grande)\n",
        "#     Muy_alto significa que el nivel del tanque está muy por encima del setpoint (error negativo grande)\n",
        "err_nivel['muy_alto'] = fuzz.trapmf(err_nivel.universe, [-1.0, -1.0, -0.8, -0.4])\n",
        "err_nivel['alto'] = fuzz.trimf(err_nivel.universe, [-0.6, -0.3, 0])\n",
        "err_nivel['ok'] = fuzz.trimf(err_nivel.universe, [-0.1, 0, 0.1])\n",
        "err_nivel['bajo'] = fuzz.trimf(err_nivel.universe, [0, 0.3, 0.6])\n",
        "err_nivel['muy_bajo'] = fuzz.trapmf(err_nivel.universe, [0.4, 0.8, 1.0, 1.0])\n",
        "\n",
        "# Para tasa_cambio_error (Negativo = nivel bajando, Positivo = nivel subiendo)\n",
        "tasa_err['aumentando_rapido'] = fuzz.trapmf(tasa_err.universe, [0.05, 0.1, 0.2, 0.2]) # Nivel subiendo rápido\n",
        "tasa_err['aumentando_lento'] = fuzz.trimf(tasa_err.universe, [0, 0.05, 0.1])\n",
        "tasa_err['estable'] = fuzz.trimf(tasa_err.universe, [-0.02, 0, 0.02])\n",
        "tasa_err['disminuyendo_lento'] = fuzz.trimf(tasa_err.universe, [-0.1, -0.05, 0])\n",
        "tasa_err['disminuyendo_rapido'] = fuzz.trapmf(tasa_err.universe, [-0.2, -0.2, -0.1, -0.05]) # Nivel bajando rápido\n",
        "\n",
        "# Para flujo_bomba\n",
        "flujo_bomba['cerrar_mucho'] = fuzz.trimf(flujo_bomba.universe, [0, 0, 25])\n",
        "flujo_bomba['cerrar_poco'] = fuzz.trimf(flujo_bomba.universe, [15, 40, 60])\n",
        "flujo_bomba['mantener'] = fuzz.trimf(flujo_bomba.universe, [40, 50, 60]) # Representa un flujo base\n",
        "flujo_bomba['abrir_poco'] = fuzz.trimf(flujo_bomba.universe, [50, 60, 85])\n",
        "flujo_bomba['abrir_mucho'] = fuzz.trimf(flujo_bomba.universe, [75, 100, 100])\n",
        "\n",
        "# Visualizar MFs (opcional)\n",
        "# err_nivel.view()\n",
        "# tasa_err.view()\n",
        "# flujo_bomba.view()\n",
        "\n",
        "# 3. Definir la Base de Reglas (Matriz de decisión simplificada)\n",
        "#       | Tasa_Cambio_Error                                                               |\n",
        "# Error | Disminuyendo Rápido | Disminuyendo Lento | Estable        | Aumentando Lento | Aumentando Rápido |\n",
        "#-------|---------------------|--------------------|----------------|------------------|-------------------|\n",
        "# Muy_Alto| Mantener            | Cerrar_Poco        | Cerrar_Mucho   | Cerrar_Mucho   | Cerrar_Mucho    |\n",
        "# Alto  | Abrir_Poco          | Mantener           | Cerrar_Poco    | Cerrar_Mucho   | Cerrar_Mucho    |\n",
        "# OK    | Abrir_Mucho         | Abrir_Poco         | Mantener       | Cerrar_Poco    | Cerrar_Mucho    |\n",
        "# Bajo  | Abrir_Mucho         | Abrir_Mucho        | Abrir_Poco     | Mantener         | Cerrar_Poco     |\n",
        "# Muy_Bajo| Abrir_Mucho         | Abrir_Mucho        | Abrir_Mucho    | Abrir_Poco       | Mantener        |\n",
        "\n",
        "# Reglas para 'Error_Nivel' es MUY_ALTO (nivel muy por encima del setpoint, error negativo grande)\n",
        "regla1 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['disminuyendo_rapido'], flujo_bomba['mantener'])\n",
        "regla2 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['disminuyendo_lento'], flujo_bomba['cerrar_poco'])\n",
        "regla3 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['estable'], flujo_bomba['cerrar_mucho'])\n",
        "regla4 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_mucho'])\n",
        "regla5 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es ALTO\n",
        "regla6 = ctrl.Rule(err_nivel['alto'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_poco'])\n",
        "regla7 = ctrl.Rule(err_nivel['alto'] & tasa_err['disminuyendo_lento'], flujo_bomba['mantener'])\n",
        "regla8 = ctrl.Rule(err_nivel['alto'] & tasa_err['estable'], flujo_bomba['cerrar_poco'])\n",
        "regla9 = ctrl.Rule(err_nivel['alto'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_mucho'])\n",
        "regla10 = ctrl.Rule(err_nivel['alto'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es OK\n",
        "regla11 = ctrl.Rule(err_nivel['ok'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
        "regla12 = ctrl.Rule(err_nivel['ok'] & tasa_err['disminuyendo_lento'], flujo_bomba['abrir_poco'])\n",
        "regla13 = ctrl.Rule(err_nivel['ok'] & tasa_err['estable'], flujo_bomba['mantener'])\n",
        "regla14 = ctrl.Rule(err_nivel['ok'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_poco'])\n",
        "regla15 = ctrl.Rule(err_nivel['ok'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es BAJO (nivel por debajo del setpoint, error positivo)\n",
        "regla16 = ctrl.Rule(err_nivel['bajo'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
        "regla17 = ctrl.Rule(err_nivel['bajo'] & tasa_err['disminuyendo_lento'], flujo_bomba['abrir_mucho'])\n",
        "regla18 = ctrl.Rule(err_nivel['bajo'] & tasa_err['estable'], flujo_bomba['abrir_poco'])\n",
        "regla19 = ctrl.Rule(err_nivel['bajo'] & tasa_err['aumentando_lento'], flujo_bomba['mantener'])\n",
        "regla20 = ctrl.Rule(err_nivel['bajo'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_poco'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es MUY_BAJO\n",
        "regla21 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
        "regla22 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['disminuyendo_lento'], flujo_bomba['abrir_mucho'])\n",
        "regla23 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['estable'], flujo_bomba['abrir_mucho'])\n",
        "regla24 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['aumentando_lento'], flujo_bomba['abrir_poco'])\n",
        "regla25 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['aumentando_rapido'], flujo_bomba['mantener'])\n",
        "\n",
        "\n",
        "lista_reglas = [\n",
        "    regla1, regla2, regla3, regla4, regla5, regla6, regla7, regla8, regla9, regla10,\n",
        "    regla11, regla12, regla13, regla14, regla15, regla16, regla17, regla18, regla19, regla20,\n",
        "    regla21, regla22, regla23, regla24, regla25\n",
        "]\n",
        "\n",
        "# 4. Crear el Sistema de Control y Simulación\n",
        "control_tanque = ctrl.ControlSystem(lista_reglas)\n",
        "simulacion_tanque = ctrl.ControlSystemSimulation(control_tanque)\n",
        "\n",
        "# 5. Proporcionar Entradas y Computar Salida\n",
        "# Caso 1: Nivel MUY BAJO (ej. error = 0.9m) y estable (tasa_err = 0)\n",
        "simulacion_tanque.input['error_nivel'] = 0.9  # Nivel 0.9m por debajo del setpoint\n",
        "simulacion_tanque.input['tasa_cambio_error'] = 0.0 # Nivel estable, no cambia\n",
        "\n",
        "simulacion_tanque.compute()\n",
        "flujo_calculado1 = simulacion_tanque.output['flujo_bomba']\n",
        "print(f\"Caso 1 (Error_Nivel=0.9, Tasa_Error=0): Flujo Bomba Sugerido = {flujo_calculado1:.2f}%\")\n",
        "# flujo_bomba.view(sim=simulacion_tanque) # Visualizar para este caso\n",
        "\n",
        "# Caso 2: Nivel OK (ej. error = 0.0m) y bajando lentamente (tasa_err = -0.04 m/s)\n",
        "simulacion_tanque.input['error_nivel'] = 0.0\n",
        "simulacion_tanque.input['tasa_cambio_error'] = -0.04 # Nivel bajando lentamente\n",
        "\n",
        "simulacion_tanque.compute()\n",
        "flujo_calculado2 = simulacion_tanque.output['flujo_bomba']\n",
        "print(f\"Caso 2 (Error_Nivel=0.0, Tasa_Error=-0.04): Flujo Bomba Sugerido = {flujo_calculado2:.2f}%\")\n",
        "# flujo_bomba.view(sim=simulacion_tanque)\n",
        "\n",
        "# Caso 3: Nivel ALTO (ej. error = -0.5m, es decir, 0.5m por encima) y subiendo lentamente (tasa_err = 0.03 m/s)\n",
        "simulacion_tanque.input['error_nivel'] = -0.5\n",
        "simulacion_tanque.input['tasa_cambio_error'] = 0.03 # Nivel subiendo lentamente\n",
        "\n",
        "simulacion_tanque.compute()\n",
        "flujo_calculado3 = simulacion_tanque.output['flujo_bomba']\n",
        "print(f\"Caso 3 (Error_Nivel=-0.5, Tasa_Error=0.03): Flujo Bomba Sugerido = {flujo_calculado3:.2f}%\")\n",
        "flujo_bomba.view(sim=simulacion_tanque) # Visualizar el último caso\n",
        "plt.title(\"Control de Nivel: Caso 3 (Nivel Alto, Subiendo Lento)\")\n",
        "plt.show()"
      ],
