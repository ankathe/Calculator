        "\n",
        "# Para flujo_bomba\n",
        "flujo_bomba['cerrar_mucho'] = fuzz.trimf(flujo_bomba.universe, [0, 0, 25])\n",
        "flujo_bomba['cerrar_poco'] = fuzz.trimf(flujo_bomba.universe, [15, 40, 60])\n",
        "flujo_bomba['mantener'] = fuzz.trimf(flujo_bomba.universe, [40, 50, 60]) # Representa un flujo base\n",
        "flujo_bomba['abrir_poco'] = fuzz.trimf(flujo_bomba.universe, [50, 60, 85])\n",
        "flujo_bomba['abrir_mucho'] = fuzz.trimf(flujo_bomba.universe, [75, 100, 100])\n",
        "\n",
        "# Visualizar MFs (opcional)\n",
        "# err_nivel.view()\n",
        "# tasa_err.view()\n",
        "# flujo_bomba.view()\n",
        "\n",
        "# 3. Definir la Base de Reglas (Matriz de decisi√≥n simplificada)\n",
        "#       | Tasa_Cambio_Error                                                               |\n",
        "# Error | Disminuyendo R√°pido | Disminuyendo Lento | Estable        | Aumentando Lento | Aumentando R√°pido |\n",
        "#-------|---------------------|--------------------|----------------|------------------|-------------------|\n",
        "# Muy_Alto| Mantener            | Cerrar_Poco        | Cerrar_Mucho   | Cerrar_Mucho   | Cerrar_Mucho    |\n",
        "# Alto  | Abrir_Poco          | Mantener           | Cerrar_Poco    | Cerrar_Mucho   | Cerrar_Mucho    |\n",
        "# OK    | Abrir_Mucho         | Abrir_Poco         | Mantener       | Cerrar_Poco    | Cerrar_Mucho    |\n",
        "# Bajo  | Abrir_Mucho         | Abrir_Mucho        | Abrir_Poco     | Mantener         | Cerrar_Poco     |\n",
        "# Muy_Bajo| Abrir_Mucho         | Abrir_Mucho        | Abrir_Mucho    | Abrir_Poco       | Mantener        |\n",
        "\n",
        "# Reglas para 'Error_Nivel' es MUY_ALTO (nivel muy por encima del setpoint, error negativo grande)\n",
        "regla1 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['disminuyendo_rapido'], flujo_bomba['mantener'])\n",
        "regla2 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['disminuyendo_lento'], flujo_bomba['cerrar_poco'])\n",
        "regla3 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['estable'], flujo_bomba['cerrar_mucho'])\n",
        "regla4 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_mucho'])\n",
        "regla5 = ctrl.Rule(err_nivel['muy_alto'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es ALTO\n",
        "regla6 = ctrl.Rule(err_nivel['alto'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_poco'])\n",
        "regla7 = ctrl.Rule(err_nivel['alto'] & tasa_err['disminuyendo_lento'], flujo_bomba['mantener'])\n",
        "regla8 = ctrl.Rule(err_nivel['alto'] & tasa_err['estable'], flujo_bomba['cerrar_poco'])\n",
        "regla9 = ctrl.Rule(err_nivel['alto'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_mucho'])\n",
        "regla10 = ctrl.Rule(err_nivel['alto'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es OK\n",
        "regla11 = ctrl.Rule(err_nivel['ok'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
        "regla12 = ctrl.Rule(err_nivel['ok'] & tasa_err['disminuyendo_lento'], flujo_bomba['abrir_poco'])\n",
        "regla13 = ctrl.Rule(err_nivel['ok'] & tasa_err['estable'], flujo_bomba['mantener'])\n",
        "regla14 = ctrl.Rule(err_nivel['ok'] & tasa_err['aumentando_lento'], flujo_bomba['cerrar_poco'])\n",
        "regla15 = ctrl.Rule(err_nivel['ok'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_mucho'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es BAJO (nivel por debajo del setpoint, error positivo)\n",
        "regla16 = ctrl.Rule(err_nivel['bajo'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
