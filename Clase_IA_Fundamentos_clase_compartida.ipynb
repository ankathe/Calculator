        "tasa_err = ctrl.Antecedent(universo_tasa_err, 'tasa_cambio_error')\n",
        "flujo_bomba = ctrl.Consequent(universo_flujo, 'flujo_bomba', defuzzify_method='centroid')\n",
        "\n",
        "# 2. Definir Funciones de Pertenencia\n",
        "# Para error_nivel (Negativo significa nivel por ENCIMA del setpoint, Positivo por DEBAJO)\n",
