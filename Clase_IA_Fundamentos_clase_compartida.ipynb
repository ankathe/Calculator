        "regla18 = ctrl.Rule(err_nivel['bajo'] & tasa_err['estable'], flujo_bomba['abrir_poco'])\n",
        "regla19 = ctrl.Rule(err_nivel['bajo'] & tasa_err['aumentando_lento'], flujo_bomba['mantener'])\n",
        "regla20 = ctrl.Rule(err_nivel['bajo'] & tasa_err['aumentando_rapido'], flujo_bomba['cerrar_poco'])\n",
        "\n",
        "# Reglas para 'Error_Nivel' es MUY_BAJO\n",
        "regla21 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['disminuyendo_rapido'], flujo_bomba['abrir_mucho'])\n",
        "regla22 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['disminuyendo_lento'], flujo_bomba['abrir_mucho'])\n",
        "regla23 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['estable'], flujo_bomba['abrir_mucho'])\n",
        "regla24 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['aumentando_lento'], flujo_bomba['abrir_poco'])\n",
        "regla25 = ctrl.Rule(err_nivel['muy_bajo'] & tasa_err['aumentando_rapido'], flujo_bomba['mantener'])\n",
        "\n",
        "\n",
        "lista_reglas = [\n",
        "    regla1, regla2, regla3, regla4, regla5, regla6, regla7, regla8, regla9, regla10,\n",
